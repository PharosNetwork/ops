    "startup_config": {
      "init_config": {
        "chain_id": "aldaba-ng",
        "data_path": "../data",
        "domain_id": "domain0",
        "service_name": "light",
        "host_ip": "47.84.7.245",
        "http_port": "18100",
        "ws_port": "18200",
        "tcp_port": "19000",
        "rpc_port": "20000",
        "mygrid_client_deploy_mode": "light",
